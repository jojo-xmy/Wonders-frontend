{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "https://wonders-backend.herokuapp.com"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://wonders-backend.herokuapp.com"
    }
  }
}